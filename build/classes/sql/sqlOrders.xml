<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="Orders">
	<sql id="selectAll">
		SELECT
		orderno,
		menuno,
		count,
		time
	</sql>
	
	<select id="getAllOrders">
		<include refid="selectAll"/>
		FROM
		Orders
	</select>

</mapper>
